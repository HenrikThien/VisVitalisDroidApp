<?xml version="1.0" encoding="UTF-8" ?>
<resources>
  <style name="SplashScreenTheme" parent="android:Theme">
    <item name="android:windowBackground">
      @drawable/splashscreen
    </item>
    <item name="android:windowNoTitle">true</item>
    <item name="android:windowIsTranslucent">false</item>
    <item name="android:windowIsFloating">false</item>
    <item name="android:backgroundDimEnabled">true</item>
  </style>

  <style name="AutoCompleteTextViewthememain" parent="android:Widget.Holo.Light.AutoCompleteTextView">
    <item name="android:dropDownSelector">@drawable/thememain_list_selector_holo_light</item>
    <item name="android:background">@drawable/thememain_edit_text_holo_light</item>
  </style>

  <style name="Buttonthememain" parent="android:Widget.Holo.Light.Button">
    <!-- <item name="android:background">@drawable/thememain_btn_default_holo_light</item> -->
  </style>

  <style name="ImageButtonthememain" parent="android:Widget.Holo.Light.ImageButton">
    <!--<item name="android:background">@drawable/thememain_btn_default_holo_light</item>-->
  </style>

  <style name="Spinnerthememain" parent="android:Widget.Holo.Light.Spinner">
    <item name="android:background">@drawable/thememain_spinner_background_holo_light</item>
    <item name="android:dropDownSelector">@drawable/thememain_list_selector_holo_light</item>
  </style>

  <style name="Theme.Main" parent="@android:style/Theme.Holo.Light">
    <item name="android:actionBarItemBackground">@drawable/selectable_background_maintheme</item>
    <item name="android:popupMenuStyle">@style/PopupMenu.Maintheme</item>
    <item name="android:dropDownListViewStyle">@style/DropDownListView.Maintheme</item>
    <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Maintheme</item>
    <item name="android:actionDropDownStyle">@style/DropDownNav.Maintheme</item>
    <item name="android:actionBarStyle">@style/ActionBar.Solid.Maintheme</item>
    <item name="android:actionModeBackground">@drawable/cab_background_top_maintheme</item>
    <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_maintheme</item>
    <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Maintheme</item>
  </style>

  <style name="ActionBar.Solid.Maintheme" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
    <item name="android:background">@drawable/ab_background_textured_maintheme</item>
    <item name="android:backgroundStacked">@drawable/ab_stacked_solid_maintheme</item>
    <item name="android:backgroundSplit">@drawable/ab_background_textured_maintheme</item>
    <item name="android:progressBarStyle">@style/ProgressBar.Maintheme</item>
  </style>

  <style name="ActionBar.Transparent.Maintheme" parent="@android:style/Widget.Holo.Light.ActionBar">
    <item name="android:background">@drawable/ab_transparent_maintheme</item>
    <item name="android:progressBarStyle">@style/ProgressBar.Maintheme</item>
  </style>

  <style name="PopupMenu.Maintheme" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
    <item name="android:popupBackground">@drawable/menu_dropdown_panel_maintheme</item>
  </style>

  <style name="DropDownListView.Maintheme" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
    <item name="android:listSelector">@drawable/selectable_background_maintheme</item>
  </style>

  <style name="ActionBarTabStyle.Maintheme" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
    <item name="android:background">@drawable/tab_indicator_ab_maintheme</item>
  </style>

  <style name="DropDownNav.Maintheme" parent="@android:style/Widget.Holo.Light.Spinner">
    <item name="android:background">@drawable/spinner_background_ab_maintheme</item>
    <item name="android:popupBackground">@drawable/menu_dropdown_panel_maintheme</item>
    <item name="android:dropDownSelector">@drawable/selectable_background_maintheme</item>
  </style>

  <style name="ProgressBar.Maintheme" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
    <item name="android:progressDrawable">@drawable/progress_horizontal_maintheme</item>
  </style>

  <style name="ActionButton.CloseMode.Maintheme" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
    <item name="android:background">@drawable/btn_cab_done_maintheme</item>
  </style>

  <style name="Theme.Main.Widget" parent="@android:style/Theme.Holo">
    <item name="android:popupMenuStyle">@style/PopupMenu.Maintheme</item>
    <item name="android:dropDownListViewStyle">@style/DropDownListView.Maintheme</item>
  </style>

  <style name="ProgressBarthememain" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
    <item name="android:progressDrawable">@drawable/thememain_progress_horizontal_holo_light</item>
    <item name="android:indeterminateDrawable">@drawable/thememain_progress_indeterminate_horizontal_holo_light</item>
  </style>

  <style name="SeekBarthememain" parent="android:Widget.Holo.Light.SeekBar">
    <item name="android:progressDrawable">@drawable/thememain_scrubber_progress_horizontal_holo_light</item>
    <item name="android:indeterminateDrawable">@drawable/thememain_scrubber_progress_horizontal_holo_light</item>
    <item name="android:thumb">@drawable/thememain_scrubber_control_selector_holo_light</item>
  </style>

  <style name="RatingBarthememain" parent="android:Widget.Holo.Light.RatingBar">
    <item name="android:progressDrawable">@drawable/thememain_ratingbar_full_holo_light</item>
    <item name="android:indeterminateDrawable">@drawable/thememain_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigthememain" parent="android:Widget.Holo.Light.RatingBar.Indicator">
    <item name="android:progressDrawable">@drawable/thememain_ratingbar_holo_light</item>
    <item name="android:indeterminateDrawable">@drawable/thememain_ratingbar_holo_light</item>
  </style>

  <style name="RatingBarSmallthememain" parent="android:Widget.Holo.Light.RatingBar.Small">
    <item name="android:progressDrawable">@drawable/thememain_ratingbar_small_holo_light</item>
    <item name="android:indeterminateDrawable">@drawable/thememain_ratingbar_small_holo_light</item>
  </style>

  <style name="Togglethememain" parent="android:Widget.Holo.Light.Button.Toggle">
    <item name="android:background">@drawable/thememain_btn_toggle_holo_light</item>
  </style>
</resources>